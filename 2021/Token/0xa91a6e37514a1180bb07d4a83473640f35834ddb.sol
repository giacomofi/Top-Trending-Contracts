ress _from, address _to, uint256 _value)\n', '    public\n', '    validRecipient(_to)\n', '    returns (bool)\n', '  {\n', '    return super.transferFrom(_from, _to, _value);\n', '  }\n', '\n', '\n', '  // MODIFIERS\n', '\n', '  modifier validRecipient(address _recipient) {\n', '    require(_recipient != address(0) && _recipient != address(this));\n', '    _;\n', '  }\n', '\n', '}']