int256) {\n', '\t\tBid storage bid = bids[getIndex(col, row)];\n', '\t\t\n', '\t\treturn (bid.bidder, bid.amount);\n', '\t}\n', '\n', '\tfunction makeBid(uint8 col, uint8 row, uint256 deadline) external payable stillValid(deadline) {\n', '\t\trequire(msg.sender == tx.origin, "EtheriaEx: tx origin must be sender"); // prevent someone from putting themselves in a position where they lose a tile\n', '\t\trequire(msg.value > 0, "EtheriaEx: Invalid bid");\n', '\t\t\n', '\t\tuint16 index = getIndex(col, row);\n', '\t\tBid storage existingbid = bids[index];\n', '\t\trequire(msg.value >= existingbid.amount.mul(101).div(100), "EtheriaEx: bid not 1% higher"); // require higher bid to be at least 1% higher\n', '\t\t\n', '\t\tpendingWithdrawals[existingbid.bidder] += existingbid.amount; // new bid is good. add amount of old (stale) bid to pending withdrawals (incl previous stale bid amounts)\n', '\t\t\n', '\t\texistingbid.bidder = msg.sender;\n', '\t\texistingbid.amount = msg.value;\n', '\t\t\n', '\t\temit EtheriaBidCreated(index, msg.sender, msg.value);\n', '\t}\n', '\n', '    // withdrawal of a still-good bid by the owner\n', '\tfunction withdrawBid(uint8 col, uint8 row, uint256 deadline) external stillValid(deadline) {\n', '\t\tuint16 index = getIndex(col, row);\n', '\t\tBid storage existingbid = bids[index];\n', '\t\trequire(msg.sender == existingbid.bidder, "EtheriaEx: not existing bidder");\n', '\n', '        // to discourage bid withdrawal, take a cut\n', '\t\tuint256 fees = existingbid.amount.mul(withdrawalPenaltyRate).div(1000);\n', '\t\tcollectedFees += fees;\n', '\t\t\n', '\t\tuint256 amount = existingbid.amount.sub(fees);\n', '\t\t\n', '\t\texistingbid.bidder = address(0);\n', '\t\texistingbid.amount = 0;\n', '\t\t\n', '\t\tpayable(msg.sender).transfer(amount);\n', '\t\t\n', '\t\temit EtheriaBidWithdrawn(index, msg.sender, existingbid.amount);\n', '\t}\n', '\n', '\tfunction acceptBid(uint8 col, uint8 row, uint256 minPrice, uint256 deadline) external stillValid(deadline) {\n', '\t\trequire(etheria.getOwner(col, row) == msg.sender, "EtheriaEx: Not tile owner");\n', '\t\t\n', '\t\tuint16 index = getIndex(col, row);\n', '        Bid storage existingbid = bids[index];\n', '\t\trequire(existingbid.amount > 0, "EtheriaEx: No bid to accept");\n', '\t\trequire(existingbid.amount >= minPrice, "EtheriaEx: min price not met");\n', '\t\t\n', '\t\tetheria.setOwner(col, row, existingbid.bidder);\n', '\t\trequire(etheria.getOwner(col, row) == existingbid.bidder, "EtheriaEx: setting owner failed");\n', '\n', '\t\t//collect fee\n', '\t\tuint256 fees = existingbid.amount.mul(feeRate).div(1000);\n', '\t\tcollectedFees += fees;\n', '\n', '        uint256 amount = existingbid.amount.sub(fees);\n', '        \n', '\t\texistingbid.bidder = address(0);\n', '\t\texistingbid.amount = 0;\n', '\t\t\n', '        pendingWithdrawals[msg.sender] += amount;\n', '        \n', '        emit EtheriaBidAccepted(index, msg.sender, existingbid.bidder, amount);\n', '    }\n', '\n', '    // withdrawal of funds on any and all stale bids that have been bested\n', '\tfunction withdraw(address payable destination) external {\n', '\t\tuint256 amount = pendingWithdrawals[msg.sender];\n', '\t\trequire(amount > 0, "EtheriaEx: no amounnt to withdraw");\n', '\t\t\n', '        // Remember to zero the pending refund before\n', '        // sending to prevent re-entrancy attacks\n', '        pendingWithdrawals[msg.sender] = 0;\n', '        destination.transfer(amount);\n', '\t}\n', '}']