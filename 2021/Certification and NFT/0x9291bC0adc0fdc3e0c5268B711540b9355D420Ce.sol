ess(sushiswapRouter),\n', '                results.feeTokenAmount\n', '            );\n', '\n', '            uint256[] memory feeTokenSwapAmounts =\n', '                sushiswapRouter.swapExactTokensForETH(\n', '                    results.feeTokenAmount,\n', '                    uint256(0),\n', '                    swapPath,\n', '                    address(this),\n', '                    deadline\n', '                );\n', '\n', '            results.feeEthAmount = results.feeEthAmount.add(\n', '                feeTokenSwapAmounts[feeTokenSwapAmounts.length - 1]\n', '            );\n', '\n', '            feeAddress.transfer(results.feeEthAmount);\n', '            user.userCollectedFees = user.userCollectedFees.add(\n', '                results.feeEthAmount\n', '            );\n', '        }\n', '\n', '        results.feeableEthForToken = results.feeableEth.div(2);\n', '        results.feeableEth = results.feeableEth.sub(results.feeableEthForToken);\n', '\n', '        swapPath[0] = weth;\n', '        swapPath[1] = token;\n', '\n', '        results.feeSwapAmounts = sushiswapRouter.swapExactETHForTokens{\n', '            value: results.feeableEthForToken\n', '        }(uint256(0), swapPath, address(this), deadline);\n', '\n', '        results.feeObtainedTokens = results.feeSwapAmounts[\n', '            results.feeSwapAmounts.length - 1\n', '        ];\n', '\n', '        IERC20(token).safeIncreaseAllowance(\n', '            address(sushiswapRouter),\n', '            results.feeObtainedTokens\n', '        );\n', '        (\n', '            results.liquidityTokenAmount,\n', '            results.liquidityEthAmount,\n', '            results.liquidity\n', '        ) = sushiswapRouter.addLiquidityETH{value: results.feeableEth}(\n', '            token,\n', '            results.feeObtainedTokens,\n', '            uint256(0),\n', '            uint256(0),\n', '            address(this),\n', '            deadline\n', '        );\n', '        results.slpAmount = results.slpAmoun