caIDs.at(uint256(randIndex));\n', '\n', '        require(presaleAlpacaIDs.remove(randID));\n', '\n', '        return randID;\n', '    }\n', '\n', '    function _receivedAlpaca(uint256[] memory ids) private {\n', '        for (uint256 i = 0; i < ids.length; i++) {\n', '            presaleAlpacaIDs.add(ids[i]);\n', '        }\n', '    }\n', '}']