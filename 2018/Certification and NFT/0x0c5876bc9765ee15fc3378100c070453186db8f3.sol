iod*24*60*60);\n', '        require(saleIsOn);\n', '        robotCoin.serviceTransfer(msg.sender, msg.value * 1000 / salePrice );\n', '    }\n', '\n', '    function transferEther(uint256 etherAmmount) public onlyOwner{ \n', '        require(this.balance >= etherAmmount); \n', '        owner.transfer(etherAmmount); \n', '    }\n', '\n', '}']