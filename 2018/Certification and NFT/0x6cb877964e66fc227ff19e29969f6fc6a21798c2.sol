}\n', '    }\n', '\n', '    function getMaxAgonId() external view returns(uint256) {\n', '        return agonArray.length - 1;\n', '    }\n', '\n', '    function getAgonIdArray(address _owner) external view returns(uint64[]) {\n', '        return ownerToAgonIdArray[_owner];\n', '    }\n', '}']
