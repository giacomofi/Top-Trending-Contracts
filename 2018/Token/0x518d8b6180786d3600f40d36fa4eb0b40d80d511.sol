unt that has already vested but hasn't been released yet.    \n", '    function releasableAmount() public view returns(uint256) {\n', '        return vestedAmount().sub(released); \n', '    }\n', '\n', '    /// @dev Calculates the amount that has already vested.\n', '    function vestedAmount() public view returns(uint256) {\n', '\n', '        uint256 currentBalance = token.balanceOf(this); \n', '        uint256 totalBalance = currentBalance.add(released); \n', '\n', '        if (now < start) {\n', '            return 0;\n', '        }\n', '        uint256 dT = now.sub(start); // time passed since start\n', '        uint256 dMonths = dT.div(30 days); // months passed \n', '\n', '        if (dMonths >= months) {\n', '            return totalBalance; // return everything if vesting period ended\n', '        } else {\n', '            return totalBalance.mul(dMonths).div(months); // ammount = total * (months passed / total months)\n', '        }\n', '\n', '    }\n', '\n', '    /// @notice Directly mints half of value and vests the other healf for the same beneficiary.\n', '    function initialMint(uint256 _cap) internal {\n', '        uint256 halfCap = _cap.div(2);\n', '        // 50% of vested amount minted in contract\n', '        token.mint(this, halfCap);\n', '        // 50% of vested amount minted directly for multisig wallet\n', '        token.mint(beneficiary, _cap.sub(halfCap));\n', '    }\n', '\n', '    /// @dev Modifier that makes sure that only beneficiary, in our case multisig wallet can call this.\n', '    modifier onlyBeneficiary() {\n', '        require(msg.sender == beneficiary);\n', '        _;\n', '    }\n', '\n', '}']